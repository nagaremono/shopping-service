    "@typescript-eslint/parser": "^4.29.1",
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "sequelize-cli": "^6.2.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@types/dotenv-safe": "^8.1.2",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "multer": "^1.4.3",
    "pg": "^8.7.1",
